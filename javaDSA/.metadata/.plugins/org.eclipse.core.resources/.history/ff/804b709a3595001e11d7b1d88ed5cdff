package binarySearchTree;

public class BinarySearchTree {

	static class Node {

		private int data;

		private Node left;

		private Node right;

		public Node() {
			this.data = 0;
			this.left = null;
			this.right = null;
		}

		public Node(int val) {
			this.data = val;
			this.left = null;
			this.right = null;
		}

	}

	private Node root;

