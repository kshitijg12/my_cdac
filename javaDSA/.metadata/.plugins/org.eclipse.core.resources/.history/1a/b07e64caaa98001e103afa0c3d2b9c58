package stackkClass;

public class Stackk {
	private int[] arr;
	private int [] max;

	private int top;
	private int top2;
   
	public Stackk(int val)
	{
		arr = new int[val];
		max = new int[val];
		top = -1;
		top2 = -1;
	}
	
	public void push(int val)
	{
		arr[++top] = val;
	}
	
	public int pop()
	{
		
		
		
	 return arr[top];
		
	}
	public int peek()
	{
		return arr[top];
		
	}
	public int getMax()
	{
		
		
		return max[top2];
	}
	
	public boolean isEmpty()
	{
		return top == -1;
	}
}
